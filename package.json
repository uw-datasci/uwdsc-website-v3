{
  "name": "uwdsc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web",
    "dev:cxc": "turbo dev --filter=cxc",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "node scripts/ui-add.js"
  },
  "devDependencies": {
    "@uwdsc/eslint-config": "workspace:*",
    "@uwdsc/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
