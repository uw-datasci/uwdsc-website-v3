{
  "name": "@uwdsc/server/web",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "db:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=src/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@supabase/supabase-js": "^2.49.0",
    "@uwdsc/server/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@uwdsc/eslint-config": "workspace:*",
    "@uwdsc/typescript-config": "workspace:*",
    "prisma": "^6.16.1",
    "typescript": "^5.9.2"
  },
  "exports": {
    "./database/*": "./src/database/*.ts",
    "./services/*": "./src/services/*.ts",
    "./repository/*": "./src/repository/*.ts",
    "./types/*": "./src/types/*.ts"
  }
}
